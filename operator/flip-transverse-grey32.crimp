crimp_primitive flip_transverse_grey32 {
    image_grey32 image
} image {
    crimp_image* result;
    int          x, y;


    result = crimp_new_like_transpose (image);

    for (y = 0; y < crimp_w (image); y++) {
        for (x = 0; x < crimp_h (image); x++) {

	    GREY32 (result, x, y) = GREY32 (image, crimp_w (image) - y - 1, crimp_h (image) - x - 1);

        }
    }

    return result;

}

