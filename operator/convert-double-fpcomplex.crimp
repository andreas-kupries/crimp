crimp_primitive convert_2complex_double {
    image_double image
} image {
    crimp_image* result;
    int          x, y;

    result = crimp_new_fpcomplex_at (crimp_x (image), crimp_y (image), crimp_w (image), crimp_h (image));

    for (y = 0; y < crimp_h (result); y++) {
        for (x = 0; x < crimp_w (result); x++) {
	    RE (result, x, y) = DOUBLEP (image, x, y);
	    IM (result, x, y) = BLACK;
        }
    }

    return result;
}

