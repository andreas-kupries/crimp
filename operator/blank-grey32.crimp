blank_grey32
int w
int h
int value

/*
 * Create a blank grey32 image (all pixels have the specified 'value').
 */

crimp_image*     result;
int              x, y;

result = crimp_new_grey32 (w, h);

if (w && h) {
    for (y = 0; y < h; y++) {
	for (x = 0; x < w; x++) {
	    GREY32 (result, x, y) = value;
	}
    }
}

Tcl_SetObjResult(interp, crimp_new_image_obj (result));
return TCL_OK;


