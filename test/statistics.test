# -*- tcl -*-
# -------------------------------------------------------------------------

source [file join \
            [file dirname [file join [pwd] [info script]]] \
            testutilities.tcl]

testsNeedTcl     8.5
testsNeedTcltest 2

support { useLocalFile synth.tcl }
support { useC [mainPath _test/lib]/crimp_core* crimp::core no }
testing { useC [mainPath _test/lib]/crimp*      crimp       no }

# -------------------------------------------------------------------------
## Statistics. Basic and calculation of otsu thresholds
## TODO: rgba, hsv
## TODO: known bug with fpcomplex basic statistics.
# -------------------------------------------------------------------------

test crimp-statistics-1.0 {image statistics, wrong\#args, not enough} -body {
    crimp statistics
} -returnCodes error -result {wrong # args: should be "crimp statistics subcommand ?argument ...?"}

test crimp-statistics-1.1 {image statistics, invalid method} -body {
    crimp statistics BOGUS
} -returnCodes error -result {unknown or ambiguous subcommand "BOGUS": must be basic, or otsu}

test crimp-statistics-1.2 {image statistics, wrong\#args, not enough} -body {
    crimp statistics basic
} -returnCodes error -result {wrong # args: should be "crimp statistics basic image"}

test crimp-statistics-1.3 {image statistics, wrong\#args, too many} -body {
    crimp statistics basic IMAGE toomuch
} -returnCodes error -result {wrong # args: should be "crimp statistics basic image"}

test crimp-statistics-1.4 {image statistics, wrong\#args, not enough} -body {
    crimp statistics otsu
} -returnCodes error -result {wrong # args: should be "crimp statistics otsu basic"}

test crimp-statistics-1.5 {image statistics, wrong\#args, not enough} -body {
    crimp statistics otsu DATA toomuch
} -returnCodes error -result {wrong # args: should be "crimp statistics otsu basic"}

