#
# Pixelwise exponentiation and logs.
#
foreach type {
    float
    double
} {
    crimp_map_pixel $type exp   {} $type { z = exp(a);   }
    crimp_map_pixel $type log   {} $type { z = log(a);   }
    crimp_map_pixel $type log10 {} $type { z = log10(a); }
}

