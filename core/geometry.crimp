geometry
Tcl_Obj* imageObj

crimp_image* image;
Tcl_Obj* list [4];

crimp_input_any (imageObj, image);

list [0] = Tcl_NewIntObj (crimp_x (image));
list [1] = Tcl_NewIntObj (crimp_y (image));
list [2] = Tcl_NewIntObj (crimp_w (image));
list [3] = Tcl_NewIntObj (crimp_h (image));

Tcl_SetObjResult (interp, Tcl_NewListObj (4, list));
return TCL_OK;


