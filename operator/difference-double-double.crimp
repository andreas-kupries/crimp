crimp_primitive difference_double_double {
    image_double imageA
    image_double imageB
} image {
    /*
     * Pixel- and channel-wise absolute difference of two images. The images have
     * to have equal dimensions.
     */

    #define BINOP(a,b) (fabs((a) - (b)))
    #include "binop_double_double_double.c"
    #undef BINOP
}

