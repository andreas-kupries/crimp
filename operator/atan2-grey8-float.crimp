crimp_primitive atan2_grey8_float {
    image_grey8 imageA
    image_float imageB
} image {
    /*
     * atan2() of all pixels of the two input images.
     */

    #define BINOP(x,y)    (atan2((x),(y) * 57.29577951308232087679))
    #define BINOP_POST(z) (((z) < 0) ? (360 + (z)) : (z))
    #include "binop_grey8_float_float.c"
}

