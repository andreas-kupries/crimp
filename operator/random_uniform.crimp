random_uniform
int width 
int height 

crimp_image* result;
int          x, y;

result = crimp_new_float (width, height);

for (y = 0; y < crimp_h (result); y++) {
    for (x = 0; x < crimp_w (result); x++) {
	FLOATP (result, x, y) =  RAND_FLOAT();
    }
}

Tcl_SetObjResult(interp, crimp_new_image_obj (result));
return TCL_OK;


