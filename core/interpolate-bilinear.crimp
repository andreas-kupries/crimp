crimp_primitive interpolate::bilinear {
    double a
    double b
    double c
    double d
    double xf
    double yf
} double {
    /* TODO: See if we can support a syntax which simply assigns the function */
    return crimp_interpolate_bilinear (a, b,
				       c, d,
				       xf, yf);
}

